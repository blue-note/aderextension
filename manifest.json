{
  "name": "Ader",
  "version": "2",
  "background": {
    "scripts": [
      "jquery.min.js",
      "toolbox.js",
      "pics/pics.js",
      "functions.js",
      "FrameTracker.js",
      "MasterFilter.js",
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "browserIcon.png",
      "38": "browserIcon.png"
    },
    "default_popup": "popup-2.html",
    "default_title": "Click to initialize"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "pics/pics.js",
        "functions.js",
        "toolbox.js",
        "aim.js",
        "exchanger.js"
      ],
      "run_at": "document_start",
      "all_frames": true
    }
  ],
  "content_security_policy": "default-src https://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js; style-src 'self' 'unsafe-inline' 'unsafe-eval'; connect-src *; img-src *; frame-src 'self' ",
  "description": "Ader: it really is all about you",
  "icons": {
    "16": "browserIcon.png",
    "48": "browserIcon.png",
    "128": "browserIcon.png"
  },
  "incognito": "spanning",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "permissions": [
    "http://*/*", "https://*/*", "contextMenus", "tabs", "idle", "webRequest", "webRequestBlocking", "webNavigation", "alarms", "storage"
  ],
  "web_accessible_resources": [
    "browserIcon.png",
    "pics/fashion/*",
    "pics/tech/*",
    "pics/mis/*",
    "jquery/css/images/ui-bg_inset-hard_100_fcfdfd_1x100.png",
    "jquery/css/images/ui-icons_056b93_256x240.png",
    "jquery/css/images/ui-icons_d8e7f3_256x240.png",
    "jquery/css/jquery-ui.custom.css",
    "jquery/css/override-page.css",
    "font/OpenSans.woff"
  ]
}
